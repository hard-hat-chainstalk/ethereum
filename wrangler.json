{
  "name": "ethereum",
  "compatibility_date": "2025-04-01",
  "main": "./server.ts",
  "assets": {
    "directory": "./build/client"
  },
  "observability": {
    "enabled": true
  },
  "upload_source_maps": true,
  "vars": {
    "API_KEY": "your-api-key", // Replace with environment variable
    "ENV": "production"
  },
  "site": {
    "bucket": "./assets", // Correct key for site bucket
    "entry_point": "./path-to-your-worker" // Correct key format
  }
